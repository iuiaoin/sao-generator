{
  "name": "<%= name %>",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "nut",
    "copy": "cp -r dist ../resources/static",
    "build": "rm -rf ../resources/static && nut --prod && npm run copy",
    "mock": "cross-env MODE=development node ./mock/server/index.js",
    "lint": "eslint src/* --fix --quiet",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "element-ui": "^2.9.1",
    "node-sass": "^4.12.0"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{vue,js,css}": [
        "eslint --fix --quiet",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "@nut-project/cli": "^0.2.2",
    "babel-eslint": "^10.0.1",
    "commander": "2.15.1",
    "cross-env": "5.1.4",
    "eslint": "4.19.1",
    "eslint-config-kaola": "0.1.3",
    "eslint-plugin-html": "4.0.2",
    "husky": "0.14.3",
    "koa": "2.5.0",
    "lint-staged": "^8.1.5",
    "oxz": "0.1.2"
  }
}
